{
    "name": "1",
    "n_gpu": 2,
    "fp16": false,
    "ema": false,

    "arch": {
        "type": "RNNModel",
        "args": {
            "input_dim" : 19,
            "lstm_dim" : 1024,
            "dense_dim" : 512,
            "logit_dim" : 256,
            "num_classes" : 1
        }
    },
    "dataset": {
        "csv_path": "/home/hana/sonnh/Google-Brain-Ventilator-Pressure-Prediction/train_kfold.csv",
        "training_batch_size": 512,
        "validate_batch_size": 512,
        "num_workers": 20,
        "cache": true
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001
        }
    },
    "train_loss": "bce_with_logits_loss",
    "val_loss": "bce_with_logits_loss",
    "metrics": [
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 30,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 200,
        "only_save_best": false,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 100,
        "tensorboard": true
    }
}
