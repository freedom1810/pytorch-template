{
    "name": "1",
    "n_gpu": 2,
    "fp16": false,
    "ema": false,

    "arch": {
        "type": "TimmBackbone",
        "args": {
            "model_name": "tf_efficientnet_b0_ns",
            "num_classes": 1,
            "inchannels": 3,
            "pretrained": true
        }
    },
    "dataset": {
        "csv_path": "/home/asilla/sonnh/liveness/data/train/kfold.csv",
        "image_dir": "/home/asilla/sonnh/liveness/data/train/images",
        "training_batch_size": 32,
        "validate_batch_size": 32,
        "num_workers": 20,
        "cache": true
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001
        }
    },
    "train_loss": "bce_with_logits_loss",
    "val_loss": "bce_with_logits_loss",
    "metrics": [
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 30,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 20,
        "only_save_best": false,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 100,
        "tensorboard": true
    }
}
